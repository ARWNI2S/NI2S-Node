<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>NI2S.Node.Runtime</PackageId>
    <Title>NI2S Node Runtime Module</Title>
    <Description>Core runtime library of NI2S Node that hosts and executes entities within a node.</Description>
    <TargetFrameworks>$(DefaultTargetFrameworks)</TargetFrameworks>
    <NI2SBuildTimeCodeGen>true</NI2SBuildTimeCodeGen>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="$(SourceRoot)src\NI2S.Node.Core\NI2S.Node.Core.csproj" />
  </ItemGroup>

</Project>
