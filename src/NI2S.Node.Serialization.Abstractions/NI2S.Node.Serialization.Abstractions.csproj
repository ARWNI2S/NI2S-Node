<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>NI2S.Node.Serialization.Abstractions</PackageId>
    <Title>NI2S Node Serialization Abstractions</Title>
    <Description>Serialization abstractions library for NI2S Node</Description>
    <TargetFrameworks>$(DefaultTargetFrameworks);$(CompatibilityTargetFrameworks)</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace>NI2S.Node</RootNamespace>
    <IsNI2SFrameworkPart>false</IsNI2SFrameworkPart>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" />
  </ItemGroup>

</Project>
