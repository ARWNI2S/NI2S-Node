<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>NI2S.Node</PackageId>
    <Title>NI2S Node Libraries</Title>
    <Description>Collection of NI2S Node libraries and files needed for the system.</Description>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <NoPackageAnalysis>true</NoPackageAnalysis>
    <IncludeSymbols>false</IncludeSymbols>
    <IncludeSource>false</IncludeSource>
    <TargetFrameworks>$(DefaultTargetFrameworks)</TargetFrameworks>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\NI2S.Node.Sdk\NI2S.Node.Sdk.csproj" />
  </ItemGroup>
</Project>
