<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>NI2S.Node.Data</PackageId>
    <Title>NI2S Node Data</Title>
    <Description>NI2S Node Data Module</Description>
    <PackageTags>$(PackageTags) Module Data</PackageTags>
    <TargetFrameworks>$(DefaultTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>NI2S.Node.Data</AssemblyName>
    <RootNamespace>NI2S.Node.Data</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="DataModuleLoader.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="$(ModuleProjectsPath)\NI2S.Node.Data.Abstractions\NI2S.Node.Data.Abstractions.csproj" />
    <ProjectReference Include="$(PackageProjectsPath)\NI2S.Node.Sdk\NI2S.Node.Sdk.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Domain\" />
  </ItemGroup>
</Project>
