<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>NI2S.Node.Presentation</PackageId>
    <Title>NI2S Node Presentation</Title>
    <Description>NI2S Node Presentation Module</Description>
    <PackageTags>$(PackageTags) Module Presentation-Layer</PackageTags>
    <TargetFrameworks>$(DefaultTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>NI2S.Node.Presentation</AssemblyName>
    <RootNamespace>NI2S.Node.Presentation</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="$(SourceRoot)src\NI2S.Node.Runtime\NI2S.Node.Runtime.csproj" />
    <ProjectReference Include="$(SourceRoot)src\Modules\NI2S.Node.Presentation.Abstractions\NI2S.Node.Presentation.Abstractions.csproj" />
  </ItemGroup>
</Project>
