<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <Title>NI2S Node Executable</Title>
    <Description>Executable file for NI2S Node.</Description>
    <TargetFrameworks>$(DefaultTargetFrameworks)</TargetFrameworks>
    <RootNamespace>NI2S.Node</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="NI2SNodeApp.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Mono.Options" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Configuration\Options\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NI2S.Node.Core\NI2S.Node.Core.csproj" />
  </ItemGroup>

</Project>
