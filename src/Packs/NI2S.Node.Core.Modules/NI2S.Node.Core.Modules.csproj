<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>NI2S.Node.Core.Modules</PackageId>
    <Title>NI2S Node Core Module Libraries</Title>
    <Description>Collection of NI2S Node Module libraries and files used by the system.</Description>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <NoPackageAnalysis>true</NoPackageAnalysis>
    <IncludeSymbols>false</IncludeSymbols>
    <IncludeSource>false</IncludeSource>
    <TargetFrameworks>$(DefaultTargetFrameworks)</TargetFrameworks>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="$(SourceRoot)src\Modules\NI2S.Node.Data\NI2S.Node.Data.csproj" />
    <ProjectReference Include="$(SourceRoot)src\Modules\NI2S.Node.Network\NI2S.Node.Network.csproj" />
  </ItemGroup>
</Project>
